<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="dhcpServerConfig.xsl"?>
<dhcpV6ServerConfig 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:noNamespaceSchemaLocation="dhcpServerConfig.xsd">
	
	<abcPolicy>true</abcPolicy>
	<serverIdOption>
		<hexValue>abcdef0123456789</hexValue>
	</serverIdOption>
	<dnsServersOption>
		<serverIpAddresses>3ffe::0001</serverIpAddresses>
		<serverIpAddresses>3ffe::0002</serverIpAddresses>
		<serverIpAddresses>3ffe::0003</serverIpAddresses>
	</dnsServersOption>
	<domainListOption>
		<domainNames>foo.com.</domainNames>
		<domainNames>bar.com.</domainNames>
		<domainNames>yuk.com.</domainNames>
	</domainListOption>
	<filterGroups>
		<name>UserClass Filter Group 1</name>
		<optionExpressions>
			<code>15</code>
			<data>
				<asciiValue>FilterUserClass</asciiValue>
			</data>
		</optionExpressions>
		<domainListOption>
			<domainNames>filter.com.</domainNames>
		</domainListOption>
	</filterGroups>
	
	<links>
		<address>2001:DB8:1::/48</address>
		<dnsServersOption>
			<serverIpAddresses>2001:DB8:1::1</serverIpAddresses>
		</dnsServersOption>
	</links>
	<links>
		<address>2001:DB8:2::/48</address>
		<dnsServersOption>
			<serverIpAddresses>2001:DB8:2::1</serverIpAddresses>
		</dnsServersOption>
	</links>
	<links>
		<address>2001:DB8:3::/48</address>
		<dnsServersOption>
			<serverIpAddresses>2001:DB8:3::1</serverIpAddresses>
		</dnsServersOption>
	</links>
	<links>
		<address>2001:DB8:4::/48</address>
		<dnsServersOption>
			<serverIpAddresses>2001:DB8:4::1</serverIpAddresses>
		</dnsServersOption>
	</links>
	<links>
		<address>2001:DB8:5::/48</address>
		<dnsServersOption>
			<serverIpAddresses>2001:DB8:5::1</serverIpAddresses>
		</dnsServersOption>
	</links>

</dhcpV6ServerConfig>
