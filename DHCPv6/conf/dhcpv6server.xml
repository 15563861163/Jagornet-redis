        </nisDomainNameOption>
        <nisPlusServersOption name="Network Information Service V2 (NIS+) Servers"/>
        <nisPlusDomainNameOption name="Network Information Service V2 (NIS+) Domain Name">
            <domainName></domainName>
        </nisPlusDomainNameOption>
        <sntpServersOption name="Simple Network Time Protocol (SNTP) Servers"/>
        <infoRefreshTimeOption name="Information Refresh Time">0</infoRefreshTimeOption>
    </filters>
    <filters>
        <name>Bob Filter</name>
        <optionExpressions>
            <code>2</code>
            <data>
                <asciiValue>Bob</asciiValue>
            </data>
        </optionExpressions>
        <preferenceOption name="Preference">0</preferenceOption>
        <dnsServersOption name="DNS Recursive Name Server">
            <serverIpAddresses>2001::1</serverIpAddresses>
