            <name>2001::1 New Filter</name>
            <optionExpressions>
                <code>2</code>
                <data>
                    <asciiValue>2001-serverid-option</asciiValue>
                </data>
            </optionExpressions>
            <preferenceOption>0</preferenceOption>
            <dnsServersOption>
                <serverIpAddresses>mydomain.com</serverIpAddresses>
            </dnsServersOption>
            <domainSearchListOption/>
