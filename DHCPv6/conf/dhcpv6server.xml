        </nisPlusDomainNameOption>
        <sntpServersOption name="Simple Network Time Protocol (SNTP) Servers"/>
        <infoRefreshTimeOption name="Information Refresh Time">0</infoRefreshTimeOption>
    </filters>
    <links>
        <name>3ffe::1</name>
        <preferenceOption name="Preference">0</preferenceOption>
        <dnsServersOption name="DNS Recursive Name Server">
            <serverIpAddresses>3ffe::1</serverIpAddresses>
