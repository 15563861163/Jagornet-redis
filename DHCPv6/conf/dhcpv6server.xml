<dhcpV6ServerConfig>
    <abcPolicy>false</abcPolicy>
    <serverIdOption name="Server Identifier">
        <hexValue>ABCDEF0123456789</hexValue>
    </serverIdOption>
    <preferenceOption name="Preference">0</preferenceOption>
    <dnsServersOption name="DNS Recursive Name Server"/>
    <domainSearchListOption name="Domain Search List"/>
    <sipServerAddressesOption name="SIP Server Addresses"/>
    <sipServerDomainNamesOption name="SIP Server Domain Names"/>
    <nisServersOption name="Network Information Service (NIS) Servers"/>
    <nisDomainNameOption name="Network Information Service (NIS) Domain Name">
        <domainName></domainName>
    </nisDomainNameOption>
    <nisPlusServersOption name="Network Information Service V2 (NIS+) Servers"/>
    <nisPlusDomainNameOption name="Network Information Service V2 (NIS+) Domain Name">
        <domainName></domainName>
    </nisPlusDomainNameOption>
    <sntpServersOption name="Simple Network Time Protocol (SNTP) Servers"/>
    <infoRefreshTimeOption name="Information Refresh Time">0</infoRefreshTimeOption>
    <filters>
        <name>Joe Filter</name>
        <optionExpressions>
            <code>1</code>
            <data>
                <asciiValue>Joe</asciiValue>
            </data>
        </optionExpressions>
        <preferenceOption name="Preference">0</preferenceOption>
        <dnsServersOption name="DNS Recursive Name Server"/>
        <domainSearchListOption name="Domain Search List"/>
        <sipServerAddressesOption name="SIP Server Addresses"/>
